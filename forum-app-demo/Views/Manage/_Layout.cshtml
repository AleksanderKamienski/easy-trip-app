@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}
<div class="jumbotron jumbotron-fluid">
    <h1 class="display-4">Manage your account</h1>
    <p class="lead">Change your account settings</p>
    <hr class="my-4">
</div>
<div>
    <div class="row">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}

